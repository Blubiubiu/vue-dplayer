{
  "name": "vue-dplayer",
  "description": "A Vue 2.x video player component based on DPlayer",
  "version": "0.0.4",
  "main": "dist/Vue-DPlayer.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "echo lol",
    "build:example": "vbuild -c vbuild.example.js",
    "build": "npm run build:cjs && npm run build:umd",
